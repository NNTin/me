{
  "builds": [
    {
      "src": "build.sh",
      "use": "@vercel/static-build",
      "config": { "distDir": "_site" }
    }
  ],
  "routes": [
    {
      "src": "^/$",
      "status": 308,
      "headers": { "Location": "/me/" }
    },
    { "src": "/me", "dest": "/me/index.html" },
    { "src": "/me/(.*)", "dest": "/me/$1" }
  ],
  "ignoreCommand": "if [ \"$VERCEL_GIT_COMMIT_REF\" = \"output\" ]; then exit 0; else exit 1; fi"
}
